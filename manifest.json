{
    "manifest_version": 2,
    "name": "Anmol Jha Launcher",
    "description": "Quick Launch Anmol Jha",
    "version": "1.0.0",
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },

    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },

    "permissions": ["activeTab"]
}